{
    "name": "php-http/helper",
    "description": "Helper classes for HTTP related data",
    "license": "MIT",
    "keywords": ["http", "helper"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Http\\Helper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Http\\Helper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
